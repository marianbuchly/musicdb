<h1>Songs</h1>
<h1>
  <%= @song.title %>
  <%= link_to "Delete", song_path( @song ), method: :delete, data: { confirm: "Are you sure?" }, class: "btn btn-default", style: "float: right" %>
  <%= link_to "Edit", edit_song_path( @song ), class: "btn btn-default", style: "float: right" %>
</h1>

<h3><%= link_to @song.artist.name, @song.artist %> </h3>

<p><%= @song.year %></p>
<p><%= @song.album %></p>
<p><%= @song.duration %></p>
